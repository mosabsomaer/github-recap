{
  "name": "github-recap",
  "version": "1.0.0",
  "description": "Libyan Developer Ecosystem Recap - GitHub Data Collector",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "github-recap": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:web": "vite build",
    "dev": "NODE_OPTIONS='--max-old-space-size=8192' vite",
    "preview": "vite preview",
    "cli:dev": "tsx src/cli.ts",
    "sync": "tsx src/cli.ts sync",
    "sync:users": "tsx src/cli.ts sync users",
    "sync:repos": "tsx src/cli.ts sync repos",
    "sync:contributions": "tsx src/cli.ts sync contributions",
    "status": "tsx src/cli.ts status",
    "reset": "tsx src/cli.ts reset",
    "analyze": "tsx src/cli.ts analyze",
    "add-user": "tsx src/cli.ts add-user",
    "clean:analyze": "tsx src/clean-locations.ts analyze",
    "clean:remove": "tsx src/clean-locations.ts remove"
  },
  "dependencies": {
    "@types/d3": "^7.4.3",
    "@vitejs/plugin-react": "^5.1.2",
    "commander": "^12.1.0",
    "d3": "^7.9.0",
    "dotenv": "^16.4.5",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-icons": "^5.5.0",
    "vite": "^7.2.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^22.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
